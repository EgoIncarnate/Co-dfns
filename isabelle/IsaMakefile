
## targets

default: Osmium
images: 
test: Osmium

all: images test


## global settings

SRC = $(ISABELLE_HOME)/src
OUT = $(ISABELLE_OUTPUT)
LOG = $(OUT)/log

USEDIR = $(ISABELLE_TOOL) usedir -v true -i true -d pdf  ## -D generated


## Osmium

Osmium: $(LOG)/HOL-Osmium.gz

$(LOG)/HOL-Osmium.gz: ## Osmium/ROOT.ML Osmium/document/root.tex Osmium/*.thy
	@$(USEDIR) HOL Osmium


## clean

clean:
	@rm -f $(LOG)/HOL-Osmium.gz
